      "server.js"
    ],
    "delay": "2500"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/STEP-tw/stratego-gamblers.git"
  },
  "keywords": [],
  "author": "gamblers",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/STEP-tw/stratego-gamblers/issues"
  },
  "homepage": "https://github.com/STEP-tw/stratego-gamblers#readme",
  "dependencies": {
    "cookie-parser": "^1.4.3",
    "express": "^4.16.2",
    "morgan": "^1.9.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "supertest": "^3.0.0"
  }
}
